{
	"manifest_version": 2,
	"name": "My First Extention",
	"description": "This extension is First",
	"version": "1.0",
	"content_scripts": [
		 {
			 "matches": ["https://www.google.com/*"],
			 "css": ["main.css"]
		 }
	 ],
	"page_action": {
		"default_icon": "img.png",
		"default_popup": "popup.html",
		"default_title": "My custom google page!"
	}
}
